// [0, 1, 1, 4, 9, 25, 64, 169, 441, 1156]
// 0
class Foo < Iter {
    #[constructor]
    fn new(self, max) {
        self.first = 0;
        self.second = 1;
        self.count = 0;
        self.max = max;
    }

    fn iter(self) {
        return self;
    }

    fn next(self) {
        if self.count == self.max {
            return StopIter.new();
        }
        self.count += 1;
        var old_first = self.first;
        self.first = self.second;
        self.second = old_first + self.first;
        return old_first;
    }
}

fn square(n) { return n * n; }

print(Foo.new(10).map(square).collect());