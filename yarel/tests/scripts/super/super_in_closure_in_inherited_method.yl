// A
// 0
class A {
  fn say(self) {
    print("A");
  }
}
class B < A {
  fn get_closure(self) {
    fn closure() {
      super.say();
    }
    return closure;
  }
  fn say(self) {
    print("B");
  }
}
#[constructor(new)]
class C < B {
  fn say(self) {
    print("C");
  }
}
C.new().get_closure()();
